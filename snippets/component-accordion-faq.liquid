<div data-id="component-accordion-faq" class="component-accordion-faq">
	<div data-id="component-accordion-faq__stub" class="component-accordion-faq__stub">
		<h2>sorry!</h2>
		<h2>we cant find a match.</h2>
		<p>Please review the word or try another search.</p>
	</div>
	{% for faq in faqs %}
		<button type="button" data-id="component-accordion-faq__item" class="component-accordion-faq__item">
			{{ faq.question }}
		</button>
		<div class="component-accordion-faq__content">
			{{ faq.answer }}
		</div>
	{% endfor %}
</div>
